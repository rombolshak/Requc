<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:model="clr-namespace:Requc.Models"
                    xmlns:viewmodel="clr-namespace:Requc.ViewModels"
                    xmlns:view="clr-namespace:Requc.Views"
                    xmlns:device="clr-namespace:Requc.Models"
                    xmlns:deviceview="clr-namespace:Requc.Views.Devices">
    
    <DataTemplate DataType="{x:Type viewmodel:SimpleProtocolViewModel}">
        <view:SimpleProtocolView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type model:SimpleProtocolAct}">
        <view:ProtocolActView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type device:AliceProtocolDevice}">
        <deviceview:Alice />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type device:BobProtocolDevice}">
        <deviceview:Bob />
    </DataTemplate>
</ResourceDictionary>