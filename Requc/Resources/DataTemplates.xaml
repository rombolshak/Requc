<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodel="clr-namespace:Requc.ViewModels"
                    xmlns:view="clr-namespace:Requc.Views"
                    xmlns:device="clr-namespace:Requc.Models.Devices"
                    xmlns:deviceview="clr-namespace:Requc.Views.Devices">
    <DataTemplate DataType="{x:Type viewmodel:TransmissionActViewModel}">
      <view:TransmissionActView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type device:LaserDevice}">
        <deviceview:Laser />
    </DataTemplate>

    <DataTemplate DataType="{x:Type device:PhaseShiftDevice}">
        <deviceview:PhaseShift />
    </DataTemplate>
</ResourceDictionary>